package sdetPrograms;

public class LongestConsecutive {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
 int i=101,count=0;
 
 if((i & (i<<1))!=0) {
	while((i & (i<<1))!=0) {
		 count++;
		i= (i<<1);
	}
	
 }
	 else {
		 System.out.println("Blank");
 }
System.out.println(count);
	}

}
